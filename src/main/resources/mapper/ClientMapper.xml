<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nd.mapper.ClientMapper">

    <select id="getByName" resultType="com.nd.entity.Client">
        select *
        from client
        where name = #{name}
    </select>
</mapper>